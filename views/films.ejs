<%- include('head') %>
<div class="container">
  <div class="flexContainer">
    <% films.forEach(function(film){ %>
    <div class="flexItem">
      <h2><%= film.filmName %> (<%=film.filmCertificate %>)</h2>
      <p><%= film.filmDescription %></p>
      <p><%= film.releaseDate %></p>
    </div>
    <% }) %>
  </div>
</div>
<%- include('footer'); %>
